  "prevention_rules": {
    "pre_execution_checks": [
      {
        "category": "data_validation",
        "check": "validate_file_format",
        "priority": "high",
        "implementation": "check_headers_and_encoding"
      },
      {
        "category": "api_integration", 
        "check": "verify_connectivity",
        "priority": "high",
        "implementation": "ping_endpoint_with_auth"
      },
      {
        "category": "file_processing",
        "check": "memory_availability",
        "priority": "medium",
        "implementation": "estimate_memory_needs"
      }
    ],
    "validation_patterns": [
      {
        "pattern": "schema_validation",
        "applies_to": ["data_files", "api_payloads"],
        "validation_steps": [
          "check_required_fields",
          "validate_data_types",
          "verify_constraints"
        ]
      }
    ],
    "recovery_strategies": [
      {
        "error_type": "network_failure",
        "strategy": "exponential_backoff_retry",
        "max_attempts": 3,
        "success_indicators": ["successful_response", "data_consistency"]
      },
      {
        "error_type": "data_corruption",
        "strategy": "fallback_to_backup",
        "validation_required": true,
        "success_indicators": ["data_integrity_check"]
      }
    ]
  },
